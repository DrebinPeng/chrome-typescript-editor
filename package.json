{
  "name": "typescript-editor",
  "version": "1.0.0",
  "description": "A chrome extension that allows you to run typescript code in browser",
  "scripts": {
    "build": "cross-env NODE_ENV=production EXTENSION=true webpack --config webpack.config.js --progress",
    "dev": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress",
    "start": "cross-env NODE_ENV=development EXTENSION=true webpack --config webpack.config.js --progress",
    "analyze": "cross-env ANALYZE=true yarn build"
  },
  "author": "RunbinPeng",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "babel-loader": "^8.2.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "file-loader": "^6.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^5.3.1",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "mini-css-extract-plugin": "^1.4.1",
    "monaco-editor-webpack-plugin": "^3.0.1",
    "node-polyfill-webpack-plugin": "^1.1.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^2.0.0",
